      ],
      "parameters": {
        "mode": "runOnce",
        "language": "javaScript",
        "jsCode": "const d=new Date();const pad=n=>String(n).padStart(2,'0');const y=d.getUTCFullYear();const m=pad(d.getUTCMonth()+1);const da=pad(d.getUTCDate());return {json:{today:`${y}-${m}-${da}`}};"
      }
    },
    {
      "id": "st",
