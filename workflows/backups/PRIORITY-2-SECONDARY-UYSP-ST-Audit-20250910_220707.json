      "parameters": {
        "mode": "runOnce",
        "language": "javaScript",
        "jsCode": "const st=$items('Sum sentCounter',0)[0].json;const ours=($items('Count SMS_Audit Sent (today)',0)||[]).length;const diff=(st.st_total||0)-ours;const top=(st.top_campaigns||[]).join(', ');\nreturn {json:{text:`SimpleTexting sent: ${st.st_total} | Our logs: ${ours} | Diff: ${diff}\\nTop campaign IDs: ${top}`}};"
