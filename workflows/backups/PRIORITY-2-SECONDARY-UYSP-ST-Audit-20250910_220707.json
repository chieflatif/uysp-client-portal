      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [
        -480,
        -80
      ],
      "parameters": {
        "method": "GET",
        "url": "=https://app2.simpletexting.com/v1/messaging/sent/list?date={{$items('Today YYYY-MM-DD',0)[0].json.today}}",
