    {
      "id": "sum",
      "name": "Sum sentCounter",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        -240,
        -80
      ],
      "parameters": {
        "mode": "runOncePerItem",
        "language": "javaScript",
        "jsCode": "const body=$json||{};const list=body.list||[];const total=list.reduce((a,c)=>a+(Number(c.sentCounter)||0),0);const top=list.slice(0,5).map(i=>i.id);return {json:{st_total:total, top_campaigns: top}};"
      }
    },
