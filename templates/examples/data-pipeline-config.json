{
  "projectName": "Marketing Data Pipeline",
  "projectDescription": "Multi-source marketing data ETL pipeline with advanced analytics and reporting",
  "projectSlug": "marketing-pipeline",
  "services": {
    "n8n": {
      "workflowId": "NEW_WORKFLOW_ID_HERE",
      "workflowName": "marketing-pipeline-main-workflow",
      "webhookPath": "marketing-webhook",
      "domain": "your-n8n-instance.com"
    },
    "airtable": {
      "baseId": "NEW_AIRTABLE_BASE_ID",
      "keyTables": ["Raw_Data", "Processed_Data", "Analytics", "Error_Log", "Job_Status"]
    },
    "integrations": [
      "hubspot",
      "salesforce", 
      "marketo",
      "google-analytics",
      "facebook-ads",
      "linkedin-ads"
    ],
    "destinations": [
      "bigquery",
      "tableau",
      "looker"
    ]
  },
  "framework": {
    "agentSystem": true,
    "mcpTools": true,
    "testingFramework": true,
    "backupSystem": true,
    "realTimeMonitoring": true
  },
  "customization": {
    "patterns": [
      "multi-source-field-normalization",
      "data-quality-validation", 
      "pipeline-error-handling",
      "batch-processing",
      "real-time-streaming",
      "cost-optimization"
    ],
    "fieldMappings": {
      "primaryEntity": "marketing_contacts",
      "keyFields": [
        "email",
        "contact_id", 
        "source_system",
        "last_activity_date",
        "engagement_score"
      ],
      "enrichmentFields": [
        "company_size",
        "industry",
        "lead_source",
        "campaign_attribution",
        "conversion_probability"
      ],
      "metricFields": [
        "clicks",
        "impressions", 
        "conversions",
        "cost_per_click",
        "return_on_ad_spend"
      ]
    },
    "testingFocus": [
      "multi_source_integration",
      "data_quality_validation",
      "pipeline_performance",
      "error_recovery",
      "cost_monitoring"
    ],
    "dataFlow": "Sources → Field Normalization → Quality Validation → Enrichment → Analytics → Destinations",
    "processingMode": "batch_and_streaming"
  },
  "compliance": {
    "gdpr": true,
    "ccpa": true,
    "dataRetention": "365_days",
    "piiHandling": "hash_sensitive_fields"
  },
  "monitoring": {
    "alerting": ["email", "slack"],
    "metrics": ["pipeline_health", "data_quality", "cost_tracking"],
    "dashboards": ["real_time", "daily_summary", "cost_analysis"]
  }
}