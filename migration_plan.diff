+#!/usr/bin/env node
+/**
+ * Migration Script: Fix Stuck Leads
+ *
+ * Purpose: One-time migration to identify and de-enroll leads who have
+ * completed their campaigns but weren't properly de-enrolled
+ *
+ * Part of: Campaign Manager Upgrade v2 - Phase 1
+ */
+
+import { drizzle } from 'drizzle-orm/node-postgres';
+import { Pool } from 'pg';
+import { sql } from 'drizzle-orm';
+import * as schema from '../src/lib/db/schema';
+
+const pool = new Pool({
+  connectionString: process.env.DATABASE_URL,
+  ssl: process.env.NODE_ENV === 'production' ? { rejectUnauthorized: false } : false,
+});
+
+const db = drizzle(pool, { schema });
+
+interface StuckLead {
+  id: string;
+  firstName: string;
+  lastName: string;
+  email: string;
+  campaignLinkId: string;
+  campaignName: string;
+  smsSequencePosition: number;
+  lastMessageAt: Date;
+  createdAt: Date;
+  booked: boolean;
+  smsStop: boolean;
+}
+
+async function migrateStuckLeads() {
+  console.log('üîç Starting migration to fix stuck leads...\n');
+
+  try {
+    // Step 1: Find all stuck leads
+    console.log('Step 1: Identifying stuck leads...');
+
+    const stuckLeads = await db.execute<StuckLead>(sql`
+      SELECT DISTINCT
+        l.id,
+        l.first_name,
+        l.last_name,
+        l.email,
+        l.campaign_link_id,
+        l.campaign_name,
+        l.sms_sequence_position,
+        l.last_message_at,
+        l.created_at,
+        l.booked,
+        l.sms_stop,
+        c.name as campaign_name_from_table,
+        COALESCE(jsonb_array_length(c.messages), 1) as total_messages
+      FROM leads l
+      LEFT JOIN campaigns c ON l.campaign_link_id = c.id
+      WHERE
+        -- Has a campaign assigned
+        l.campaign_link_id IS NOT NULL
+        -- Has received at least one message
+        AND l.sms_sequence_position >= 1
+        -- Not already marked as completed
+        AND l.completed_at IS NULL
+        -- Lead is still active
+        AND l.is_active = true
+        -- Has been in campaign for more than 7 days (likely stuck)
+        AND l.created_at < NOW() - INTERVAL '7 days'
+        -- Either reached max messages or campaign is invalid/missing
+        AND (
+          c.id IS NULL -- Campaign no longer exists
+          OR l.sms_sequence_position >= COALESCE(jsonb_array_length(c.messages), 1)
+          OR (c.message_template IS NOT NULL AND l.sms_sequence_position >= 1) -- Legacy single message
+        )
+      ORDER BY l.created_at
+    `);
+
+    console.log(`Found ${stuckLeads.rows.length} stuck leads\n`);
+
+    if (stuckLeads.rows.length === 0) {
+      console.log('‚ú® No stuck leads found! System is clean.');
+      return {
+        success: true,
+        totalMigrated: 0,
+        message: 'No stuck leads found'
+      };
+    }
+
+    // Step 2: Analyze stuck leads by category
+    console.log('Step 2: Analyzing stuck leads by category...\n');
+
+    const categories = {
+      missingCampaign: [] as StuckLead[],
+      completedSequence: [] as StuckLead[],
+      legacyComplete: [] as StuckLead[],
+      longIdle: [] as StuckLead[]
+    };
+
+    for (const lead of stuckLeads.rows) {
+      if (!lead.campaign_name_from_table) {
+        categories.missingCampaign.push(lead);
+      } else if (lead.sms_sequence_position >= lead.total_messages) {
+        categories.completedSequence.push(lead);
+      } else if (lead.lastMessageAt && new Date(lead.lastMessageAt) < new Date(Date.now() - 30 * 24 * 60 * 60 * 1000)) {
+        categories.longIdle.push(lead);
+      } else {
+        categories.legacyComplete.push(lead);
+      }
+    }
+
+    console.log('Categories breakdown:');
+    console.log(`  üì≠ Missing campaign (deleted): ${categories.missingCampaign.length}`);
+    console.log(`  ‚úÖ Completed sequence: ${categories.completedSequence.length}`);
+    console.log(`  ‚è∞ Idle > 30 days: ${categories.longIdle.length}`);
+    console.log(`  üìú Legacy format: ${categories.legacyComplete.length}\n`);
+
+    // Step 3: Migrate each category
+    console.log('Step 3: Migrating stuck leads...\n');
+
+    let totalMigrated = 0;
+
+    // Process each category
+    for (const [category, leads] of Object.entries(categories)) {
+      if (leads.length === 0) continue;
+
+      console.log(`Processing ${category}: ${leads.length} leads`);
+
+      for (const lead of leads) {
+        // Determine outcome
+        let outcome = 'completed';
+        if (lead.booked) outcome = 'booked';
+        else if (lead.smsStop) outcome = 'opted_out';
+        else if (category === 'missingCampaign') outcome = 'campaign_deleted';
+        else if (category === 'longIdle') outcome = 'abandoned';
+
+        // Build history entry
+        const historyEntry = {
+          campaignId: lead.campaignLinkId,
+          campaignName: lead.campaignName || 'Unknown Campaign',
+          enrolledAt: lead.createdAt,
+          completedAt: new Date(),
+          messagesReceived: lead.smsSequencePosition,
+          outcome: outcome,
+          migrationNote: `Migrated from stuck state: ${category}`
+        };
+
+        // De-enroll the lead
+        await db.execute(sql`
+          UPDATE leads
+          SET
+            campaign_link_id = NULL,
+            campaign_name = NULL,
+            completed_at = COALESCE(${lead.lastMessageAt}, NOW()),
+            campaign_history = COALESCE(campaign_history, '[]'::jsonb) || ${JSON.stringify([historyEntry])}::jsonb,
+            updated_at = NOW()
+          WHERE id = ${lead.id}
+        `);
+
+        console.log(`  ‚úì Migrated: ${lead.firstName} ${lead.lastName} (${lead.email}) - ${outcome}`);
+        totalMigrated++;
+      }
+    }
+
+    // Step 4: Update campaign stats for affected campaigns
+    console.log('\nStep 4: Updating campaign statistics...');
+
+    const affectedCampaignIds = [...new Set(stuckLeads.rows
+      .map(l => l.campaignLinkId)
+      .filter(id => id))];
+
+    for (const campaignId of affectedCampaignIds) {
+      await db.execute(sql`
+        UPDATE campaigns
+        SET
+          active_leads_count = (
+            SELECT COUNT(*) FROM leads
+            WHERE campaign_link_id = ${campaignId}
+              AND completed_at IS NULL
+          ),
+          completed_leads_count = (
+            SELECT COUNT(*) FROM leads
+            WHERE campaign_history::text LIKE '%' || ${campaignId} || '%'
+          ),
+          updated_at = NOW()
+        WHERE id = ${campaignId}
+      `);
+    }
+
+    // Step 5: Generate report
+    console.log('\nüìä Migration Report:');
+    console.log('='.repeat(50));
+    console.log(`Total stuck leads found: ${stuckLeads.rows.length}`);
+    console.log(`Total leads migrated: ${totalMigrated}`);
+    console.log(`Campaigns affected: ${affectedCampaignIds.length}`);
+
+    // Log to activity log
+    await db.execute(sql`
+      INSERT INTO activity_log (user_id, action, details, ip_address)
+      VALUES (
+        NULL,
+        'STUCK_LEADS_MIGRATION',
+        ${JSON.stringify({
+          timestamp: new Date(),
+          totalFound: stuckLeads.rows.length,
+          totalMigrated,
+          campaignsAffected: affectedCampaignIds.length,
+          categories: Object.entries(categories).map(([name, leads]) => ({
+            name,
+            count: leads.length
+          }))
+        })}::jsonb,
+        '127.0.0.1'
+      )
+    `);
+
+    console.log('\n‚ú® Migration completed successfully!');
+
+    return {
+      success: true,
+      totalMigrated,
+      breakdown: Object.entries(categories).map(([name, leads]) => ({
+        category: name,
+        count: leads.length
+      }))
+    };
+
+  } catch (error) {
+    console.error('‚ùå Migration error:', error);
+
+    // Log error
+    await db.execute(sql`
+      INSERT INTO activity_log (user_id, action, details, ip_address)
+      VALUES (
+        NULL,
+        'STUCK_LEADS_MIGRATION_ERROR',
+        ${JSON.stringify({
+          timestamp: new Date(),
+          error: error.message,
+          stack: error.stack
+        })}::jsonb,
+        '127.0.0.1'
+      )
+    `);
+
+    return {
+      success: false,
+      error: error.message
+    };
+
+  } finally {
+    await pool.end();
+  }
+}
+
+// Execute if called directly
+if (require.main === module) {
+  console.log('üöÄ Starting stuck leads migration...\n');
+  console.log('This is a ONE-TIME migration to fix leads stuck in campaigns.\n');
+
+  migrateStuckLeads()
+    .then(result => {
+      console.log('\nüìã Final Result:', JSON.stringify(result, null, 2));
+      process.exit(result.success ? 0 : 1);
+    })
+    .catch(error => {
+      console.error('Fatal error:', error);
+      process.exit(1);
+    });
+}
+
+export { migrateStuckLeads };
